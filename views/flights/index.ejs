<%- include('../partials/header') %>
<h1>Flight List</h1>
<table>
  <thead>
    <tr>
      <th>Airport</th>
      <th>Airline</th>
      <th>Flight Number</th>
      <th>Departs</th>
      <th>Details</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(f) { %>
    <tr>
      <td><%= f.airport %></td>
      <td><%= f.airline %></td>
      <td><%= f.flightNo %></td>
      <td><%= f.departs %></td>
      <td><a href="/flights/<%= f._id %>">Details</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>
</body>

</html>
</head>

</html>