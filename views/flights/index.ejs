<%- include('../partials/header') %>
    <table>
      <thead>
        <tr>
          <th>Airline</th>
          <th>Flight Number</th>
          <th>Departs</th>
        </tr>
      </thead>
      <tbody>
        <% flights.forEach(function(f){ %>
        <tr>
          <td><%= f.airline %></td>
          <td><%= f.flightNo %></td>
          <td><%= f.departs %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
